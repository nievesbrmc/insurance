    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:WpfApp1">
    <SolidColorBrush  x:Key="Title" Color="#255AA8"/>
    <SolidColorBrush x:Key="Text" Color="#1B1A16" />
    <SolidColorBrush x:Key="SubText" Color="#595955" />


    <SolidColorBrush x:Key="GrayLabel" Color="#777673" />
